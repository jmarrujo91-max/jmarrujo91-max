                        app: "language",
                        code: "en"
                    }).catch((t=>(console.warn("🦉 Helperbird An error occurred while sending message to background script:", t),
                    [])));
                    if (this.languages.all = e,
                    !Array.isArray(this.languages.all) || 0 === this.languages.all.length)
                        return !1;
                    if (this.settings.position = this.database.hasProperty("predictionLocation", "bottom-0"),
                    !this.isEnabled && !t)
                        return this.off(),
                        !1;
                    this.languages.chosen = this.database.hasProperty("predictionLanguage", "DICT_EN");
                    const n = this.languages.all.find((t=>t.code === this.languages.chosen));
                    return n && (this.languages.database = n.database),
                    this.isEnabled && t ? (this.predictionary = So.instance(),
                    this.parseWords(this.languages.database, this.languages.chosen.toString()),
                    this.predictionary.useDictionaries([this.languages.chosen.toString()]),
                    this.on()) : this.off(),
                    this.isEnabled
                }
                ))
            }
            apply() {
                return Do(this, null, (function*() {
                    yield this.applyClasses("input"),
                    yield this.applyClasses("textarea")
                }
                ))
            }
            refreshSuggestions() {
                const t = document.activeElement;
                if (this.isGoogleDocs()) {
                    this.shadow.google = this.shadow.google.replace(/\n|\r/g, "");
                    const t = this.shadow.google.split(" ").pop().trim()
                      , e = this.predictionary.predict(t, {
                        maxPredictions: this.prediction.nrOfSuggestions,
                        applyToInput: !1
                    });
                    this.prediction.suggestions = e
                } else
                    this.prediction.suggestions = this.predictionary.predict(t.value.trim(), {
                        maxPredictions: this.prediction.nrOfSuggestions,
                        applyToInput: !1
                    });
                0 === this.prediction.suggestions.length ? this.prediction.index = null : (this.prediction.index = 0,
                this.off(),
                this.injectBar())
            }
            isGoogleDocs() {
                return window.location.href.indexOf("docs.google.com/document") > -1
            }
            add(t, e) {
                if (pt(t))
                    return !1;
                const n = this.isGoogleDocs();
                try {
                    if (n ? window.postMessage({
                        type: "helperbirdWord",
                        text: e,
                        lastWord: t
                    }, "*") : (t.value = this.predictionary.applyPrediction(t.value.trim(), e),
                    t.focus()),
                    this.prediction.index = null,
                    this.refreshSuggestions(),
                    !this.learn.fromChosen)
                        return;
                    const r = n ? this.shadow.google.replace(/\n|\r/g, "") : t.value;
                    this.predictionary.learnFromInput(r)
                } catch (t) {
                    console.warn("🦉 Helperbird error", t)
                }
            }
            createButtons() {
                return this.prediction.suggestions.map(((t,e)=>`\n      <span class="tw-helperbird-app-join">\n        <button type="button" aria-label="Select ${t}" title="Select ${t}" id="helperbirdwordbutton${e}" class="tw-helperbird-app-btn tw-helperbird-app-join-item">\n          ${t}\n        </button>\n        <button type="button" id="helperbirdwordbuttonspeak${e}" aria-label="Read ${t}" helperbird="${t}" title="Read ${t}" class="tw-helperbird-app-btn tw-helperbird-app-join-item">\n     \n\n<svg xmlns="http://www.w3.org/2000/svg" class="tw-h-5 tw-w-5" viewBox="0 0 24 24" fill="currentColor"><path d="M0 0h24v24H0z" fill="none"/><path d="M3 9v6h4l5 5V4L7 9H3zm13.5 3c0-1.77-1.02-3.29-2.5-4.03v8.05c1.48-.73 2.5-2.25 2.5-4.02zM14 3.23v2.06c2.89.86 5 3.54 5 6.71s-2.11 5.85-5 6.71v2.06c4.01-.91 7-4.49 7-8.77s-2.99-7.86-7-8.77z"/></svg>\n        </button>\n      </span>\n    `)).join("")
            }
            eventListener() {
                this.handlers.google = t=>{
                    if ("helperbirdGoogledocs" === t.data.type)
                        try {
                            this.shadow.google = t.data.text,
                            this.refreshSuggestions()
                        } catch (e) {
                            console.warn(`Helperbird error during processing event data: ${JSON.stringify(t.data)}. Error: ${e}`)
                        }
                }
                ,
                window.addEventListener("message", this.handlers.google)
            }
            injectBar() {
                return Do(this, null, (function*() {
                    let t = yield this.createButtons();
                    this.shadow.element = document.createElement("helperbird-word-prediction"),
                    this.shadow.element.attachShadow({
                        mode: "open"
                    }),
                    this.shadow.element.id = this.settings.id,
                    this.shadow.element.shadowRoot.innerHTML = `\n        <!DOCTYPE html>\n          <html>\n            <head class="helperbird-root-hd">\n              <meta charset="utf-8" />\n              <title>Helperbird: Voice Typing</title>\n              <style id="helperbird-word-prediction">\t\n\n\n              \n\n\n                ${ve.toString()}\n            \n                .tw-cool-bottom {\n                  bottom: 5.5rem !important;\n                  z-index: 99999999 !important;\n                  min-width:440px !important;\n                }\n             \n\n                \n            </style>\n\n        \n            </head>\n\n            <body class="helperbird-root-tw">\n\n\n\n            <div class="tw-fixed tw-inset-x-0 tw-bottom-0 tw-pb-2 tw-sm:tw-pb-5  tw-cool-bottom-two " style="\n            z-index: 99999;\n        ">\n            <div class="tw-px-10 tw-mx-auto tw-max-w-4xl tw-pointer-events-all">\n                <div \n                    class="\n                      tw-helperbird-app-card tw-shadow-lg tw-helperbird-app-compact tw-side  tw-bg-base-300 tw-col-span-6\n                    " \n                \n    \n                    >\n                    <div  class="tw-helperbird-app-card-body">\n                  <div class="tw-flex tw-flex-wrap tw-items-center tw-justify-between">\n                    <div class="tw-flex tw-w-0 tw-flex-1 tw-items-center">\n                     \n                      <div class="tw-ml-3 tw-truncate tw-font-medium tw-overflow-auto">\n                       ${t}\n                      </div>\n                    </div>\n              \n                    <div class="tw-order-2 tw-flex-shrink-0 tw-sm:tw-order-3 tw-sm:tw-ml-2">\n                    <div\n                      class="tw-helperbird-app-tooltip tw-helperbird-app-secondary tw-helperbird-app-tooltip-top"\n                      data-tip="Close voice typing"\n                    >\n                      <button\n                        type="button"\n                        id="close_banner"\n                        aria-label="Close Word prediction"\n                        class=" tw-helperbird-app-btn tw-helperbird-app-btn-ghost tw-helperbird-app-btn-sm tw-helperbird-app-btn-square"\n                      >\n                        <span class="tw-sr-only">Close Word prediction</span>\n                  \n                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="#ffffff" class="tw-w-6 tw-h-6">\n                          <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12" />\n                        </svg>\n\n                  \n                      </button>\n                      </div>\n                    </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n\n            \n\n\n\n            </body>\n          </html>\n\n\n\n\n\n\n\n    \n\n\n\t\n\n\t\t`,
                    document.body.insertBefore(this.shadow.element, document.body.firstChild);
                    const e = (t,e,n)=>{
                        this.shadow.element.shadowRoot.getElementById(t).addEventListener(e, n)
                    }
                    ;
                    e("close_banner", "click", (()=>{
                        Et(this.settings.id)
                    }
                    ));
                    for (let[t,n] of this.prediction.suggestions.entries()) {
                        const r = `helperbirdwordbutton${t}`;
                        e(r, "mousedown", (t=>{
                            this.shadow.last = this.isGoogleDocs() ? this.shadow.google : document.activeElement,
                            t.preventDefault(),
                            t.returnValue = !0
                        }
                        )),
                        e(r, "click", (()=>{
                            this.add(this.shadow.last, n)
                        }
                        ));
                        const i = `helperbirdwordbuttonspeak${t}`;
                        e(i, "mousedown", (t=>{
                            t.preventDefault(),
                            t.returnValue = !0
                        }
                        )),
                        e(i, "click", (t=>Do(this, null, (function*() {
                            t.preventDefault(),
                            t.returnValue = !1;
                            let e = t.currentTarget.getAttribute("helperbird");
                            try {
                                if (this.isSpeaking)
                                    return;
                                yield this.speak(e)
                            } catch (t) {
                                console.warn("🦉 Helperbird  error", t)
                            }
                        }
                        ))))
                    }
                    this.isGoogleDocs() ? this.eventListener() : this.applyEventListners()
                }
                ))
            }
            speak(t) {
                return Do(this, null, (function*() {
                    this.isSpeaking = !0,
                    this.TTS_ENGINE.speak(t),
                    this.isSpeaking = !1
                }
                ))
            }
            applyClasses(t) {
                return Do(this, null, (function*() {
                    let e = document.querySelectorAll(t)
                      , n = [];
                    for (let t of e)
                        "text" !== t.type && "textarea" !== t.type && "search" !== t.type || !1 !== t.hasAttribute("style") || (t.classList.add("helperbird-word-prediction-input"),
                        n.push(t));
                    return n
                }
                ))
            }
            enabled() {
                return this.isEnabled
            }
            parseWords(t, e) {
                this.predictionary.parseWords(t, {
                    elementSeparator: "\n",
                    rankSeparator: " ",
                    wordPosition: 2,
                    rankPosition: 0,
                    addToDictionary: e
                })
            }
            on() {
                return Do(this, null, (function*() {
                    this.off(),
                    !0 === this.isGoogleDocs() ? (!function(t, e) {
                        let n = document.createElement("script");
                        n.setAttribute("id", e),
                        n.src = chrome.runtime.getURL(t),
                        (document.head || document.documentElement).appendChild(n)
                    }("/scripts/google.js", "helperbird-robert-gabriel"),
                    this.eventListener()) : (yield this.apply(),
                    yield this.applyEventListners()),
                    this.prediction.suggestions.length > 0 && this.injectBar()
                }
                ))
            }
            applyEventListners() {
                return Do(this, null, (function*() {
                    let t = this;
                    [].forEach.call(this.shadow.inputs, (e=>{
                        t.handlers.keypress = ()=>{
                            t.timer && window.clearTimeout(t.timer),
                            t.timer = window.setTimeout((()=>{
                                try {
                                    t.predictionary.learnFromInput(e.value),
                                    t.refreshSuggestions()
                                } catch (t) {
                                    console.warn("🦉 Helperbird  error", t)
                                }
                            }
                            ), t.timer.timeout)
                        }
                        ,
                        e.addEventListener("keypress", this.handlers.keypress),
                        this.handlers.blur = t=>{
                            setTimeout((()=>{
                                if (null !== t.relatedTarget) {
                                    if (!0 === t.relatedTarget.classList.contains("word_button_helperbird"))
                                        return !1
                                } else
                                    Et(this.settings.id)
                            }
                            ), 4e4)
                        }
                        ,
                        e.addEventListener("blur", this.handlers.blur)
                    }
                    ))
                }
                ))
            }
            off() {
                yt(this.settings.id) && (window.removeEventListener("message", this.handlers.google),
                this.isGoogleDocs() || [].forEach.call(this.shadow.inputs, (t=>{
                    this.handlers.keypress && t.removeEventListener("keypress", this.handlers.keypress),
                    this.handlers.blur && t.removeEventListener("blur", this.handlers.blur)
                }
                )),
                Et(this.settings.id))
            }
        }
        var Lo = (t,e,n)=>new Promise(((r,i)=>{
            var o = t=>{
                try {
                    s(n.next(t))
                } catch (t) {
                    i(t)
                }
            }
              , a = t=>{
                try {
                    s(n.throw(t))
                } catch (t) {
                    i(t)
                }
            }
              , s = t=>t.done ? r(t.value) : Promise.resolve(t.value).then(o, a);
            s((n = n.apply(t, e)).next())
        }
        ));
        class Io extends ze {
            constructor(t, e) {
                super(),
                this.shortcut = e,
                this.multiplier = "10",
                this.id = "helperbird-zoom-id",
                this.key = "enableZoom",
                this.isEnabled = !1,
                this.database = t,
                this.isShortcutEnabled = !1,
                this.isPaid = !1
            }
            load(t=!1, e=!1) {
                return Lo(this, null, (function*() {
                    if (this.isPaid = e,
                    this.isEnabled = this.database.hasProperty(this.key, !1),
                    this.multiplier = this.database.hasProperty("fontZoom", this.multiplier),
                    this.isShortcutEnabled = this.database.hasProperty("enableShortcuts", !1),
                    !0 === this.isShortcutEnabled) {
                        let e = yield st(this.key);
                        this.shortcut(`${e}`, (e=>Lo(this, null, (function*() {
                            e.preventDefault(),
                            this.isEnabled = !this.isEnabled,
                            !0 === this.isEnabled && !0 === t ? this.on() : this.off()
                        }
                        ))))
                    }
                    return this.isEnabled && !0 === t ? this.on() : this.off(),
                    new Promise((t=>{
                        t(this.isEnabled)
                    }
                    ))
                }
                ))
            }
            enabled() {
                return Lo(this, null, (function*() {
                    return new Promise((t=>{
                        t(this.isEnabled)
                    }
                    ))
                }
                ))
            }
            on() {
                this.off();
                const t = ` \n\t\tbody{\n\t\t\tzoom: ${this.multiplier}%;\n\t\t\t-moz-transform: scale(${this.multiplier}%); \n\t\t\t-moz-transform-origin: 0 0;\n\n\t\t}`;
                xt(this.id, t)
            }
            off() {
                !0 === yt(this.id) && Et(this.id)
            }
        }
        var Ro = Object.getPrototypeOf
          , Po = Reflect.get
          , Oo = (t,e,n)=>Po(Ro(t), n, e)
          , Fo = (t,e,n)=>new Promise(((r,i)=>{
            var o = t=>{
                try {
                    s(n.next(t))
                } catch (t) {
                    i(t)
                }
            }
              , a = t=>{
                try {
                    s(n.throw(t))
                } catch (t) {
                    i(t)
                }
            }
              , s = t=>t.done ? r(t.value) : Promise.resolve(t.value).then(o, a);
            s((n = n.apply(t, e)).next())
        }
        ));
        class Mo extends ze {
            constructor(t=!1, e=!1) {
