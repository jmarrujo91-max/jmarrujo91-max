        function Kt(t, e, n) {
            for (var r = 0; r < t.length; r++) {
                var i = t[r];
                if (Yt(i, e, n))
                    return i
            }
        }
        function Yt(t, e, n) {
            var r = t.filter;
            if ("string" == typeof r) {
                if (r === e.nodeName.toLowerCase())
                    return !0
            } else if (Array.isArray(r)) {
                if (r.indexOf(e.nodeName.toLowerCase()) > -1)
                    return !0
            } else {
                if ("function" != typeof r)
                    throw new TypeError("`filter` needs to be a string, array, or function");
                if (r.call(t, e, n))
                    return !0
            }
        }
        function Xt(t) {
            var e = t.nextSibling || t.parentNode;
            return t.parentNode.removeChild(t),
            e
        }
        function Zt(t, e, n) {
            return t && t.parentNode === e || n(e) ? e.nextSibling || e.parentNode : e.firstChild || e.nextSibling || e.parentNode
        }
        Gt.paragraph = {
            filter: "p",
            replacement: function(t) {
                return "\n\n" + t + "\n\n"
            }
        },
        Gt.lineBreak = {
            filter: "br",
            replacement: function(t, e, n) {
                return n.br + "\n"
            }
        },
        Gt.heading = {
            filter: ["h1", "h2", "h3", "h4", "h5", "h6"],
            replacement: function(t, e, n) {
                var r = Number(e.nodeName.charAt(1));
                return "setext" === n.headingStyle && r < 3 ? "\n\n" + t + "\n" + Mt(1 === r ? "=" : "-", t.length) + "\n\n" : "\n\n" + Mt("#", r) + " " + t + "\n\n"
            }
        },
        Gt.blockquote = {
            filter: "blockquote",
            replacement: function(t) {
                return "\n\n" + (t = (t = t.replace(/^\n+|\n+$/g, "")).replace(/^/gm, "> ")) + "\n\n"
            }
        },
        Gt.list = {
            filter: ["ul", "ol"],
            replacement: function(t, e) {
                var n = e.parentNode;
                return "LI" === n.nodeName && n.lastElementChild === e ? "\n" + t : "\n\n" + t + "\n\n"
            }
        },
        Gt.listItem = {
            filter: "li",
            replacement: function(t, e, n) {
                t = t.replace(/^\n+/, "").replace(/\n+$/, "\n").replace(/\n/gm, "\n    ");
                var r = n.bulletListMarker + "   "
                  , i = e.parentNode;
                if ("OL" === i.nodeName) {
                    var o = i.getAttribute("start")
                      , a = Array.prototype.indexOf.call(i.children, e);
                    r = (o ? Number(o) + a : a + 1) + ".  "
                }
                return r + t + (e.nextSibling && !/\n$/.test(t) ? "\n" : "")
            }
        },
        Gt.indentedCodeBlock = {
            filter: function(t, e) {
                return "indented" === e.codeBlockStyle && "PRE" === t.nodeName && t.firstChild && "CODE" === t.firstChild.nodeName
            },
            replacement: function(t, e, n) {
                return "\n\n    " + e.firstChild.textContent.replace(/\n/g, "\n    ") + "\n\n"
            }
        },
        Gt.fencedCodeBlock = {
            filter: function(t, e) {
                return "fenced" === e.codeBlockStyle && "PRE" === t.nodeName && t.firstChild && "CODE" === t.firstChild.nodeName
            },
            replacement: function(t, e, n) {
                for (var r, i = ((e.firstChild.getAttribute("class") || "").match(/language-(\S+)/) || [null, ""])[1], o = e.firstChild.textContent, a = n.fence.charAt(0), s = 3, l = new RegExp("^" + a + "{3,}","gm"); r = l.exec(o); )
                    r[0].length >= s && (s = r[0].length + 1);
                var c = Mt(a, s);
                return "\n\n" + c + i + "\n" + o.replace(/\n$/, "") + "\n" + c + "\n\n"
            }
        },
        Gt.horizontalRule = {
            filter: "hr",
            replacement: function(t, e, n) {
                return "\n\n" + n.hr + "\n\n"
            }
        },
        Gt.inlineLink = {
            filter: function(t, e) {
                return "inlined" === e.linkStyle && "A" === t.nodeName && t.getAttribute("href")
            },
            replacement: function(t, e) {
                var n = e.getAttribute("href")
                  , r = Vt(e.getAttribute("title"));
                return r && (r = ' "' + r + '"'),
                "[" + t + "](" + n + r + ")"
            }
        },
        Gt.referenceLink = {
            filter: function(t, e) {
                return "referenced" === e.linkStyle && "A" === t.nodeName && t.getAttribute("href")
            },
            replacement: function(t, e, n) {
                var r, i, o = e.getAttribute("href"), a = Vt(e.getAttribute("title"));
                switch (a && (a = ' "' + a + '"'),
                n.linkReferenceStyle) {
                case "collapsed":
                    r = "[" + t + "][]",
                    i = "[" + t + "]: " + o + a;
                    break;
                case "shortcut":
                    r = "[" + t + "]",
                    i = "[" + t + "]: " + o + a;
                    break;
                default:
                    var s = this.references.length + 1;
                    r = "[" + t + "][" + s + "]",
                    i = "[" + s + "]: " + o + a
                }
                return this.references.push(i),
                r
            },
            references: [],
            append: function(t) {
                var e = "";
                return this.references.length && (e = "\n\n" + this.references.join("\n") + "\n\n",
                this.references = []),
                e
            }
        },
        Gt.emphasis = {
            filter: ["em", "i"],
            replacement: function(t, e, n) {
                return t.trim() ? n.emDelimiter + t + n.emDelimiter : ""
            }
        },
        Gt.strong = {
            filter: ["strong", "b"],
            replacement: function(t, e, n) {
                return t.trim() ? n.strongDelimiter + t + n.strongDelimiter : ""
            }
        },
        Gt.code = {
            filter: function(t) {
                var e = t.previousSibling || t.nextSibling
                  , n = "PRE" === t.parentNode.nodeName && !e;
                return "CODE" === t.nodeName && !n
            },
            replacement: function(t) {
                if (!t)
                    return "";
                t = t.replace(/\r?\n|\r/g, " ");
                for (var e = /^`|^ .*?[^ ].* $|`$/.test(t) ? " " : "", n = "`", r = t.match(/`+/gm) || []; -1 !== r.indexOf(n); )
                    n += "`";
                return n + e + t + e + n
            }
        },
        Gt.image = {
            filter: "img",
            replacement: function(t, e) {
                var n = Vt(e.getAttribute("alt"))
                  , r = e.getAttribute("src") || ""
                  , i = Vt(e.getAttribute("title"));
                return r ? "![" + n + "](" + r + (i ? ' "' + i + '"' : "") + ")" : ""
            }
        },
        Wt.prototype = {
            add: function(t, e) {
                this.array.unshift(e)
            },
            keep: function(t) {
                this._keep.unshift({
                    filter: t,
                    replacement: this.keepReplacement
                })
            },
            remove: function(t) {
                this._remove.unshift({
                    filter: t,
                    replacement: function() {
                        return ""
                    }
                })
            },
            forNode: function(t) {
                return t.isBlank ? this.blankRule : (e = Kt(this.array, t, this.options)) || (e = Kt(this._keep, t, this.options)) || (e = Kt(this._remove, t, this.options)) ? e : this.defaultRule;
                var e
            },
            forEach: function(t) {
                for (var e = 0; e < this.array.length; e++)
                    t(this.array[e], e)
            }
        };
