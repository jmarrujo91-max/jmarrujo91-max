                            i.checked = -1 !== o.indexOf(this.customClassOption)
                        }
                    },
                    destroy: function() {
                        if (!this.form)
                            return !1;
                        this.form.parentNode && this.form.parentNode.removeChild(this.form),
                        delete this.form
                    },
                    getFormOpts: function() {
                        var t = this.getAnchorTargetCheckbox()
                          , e = this.getAnchorButtonCheckbox()
                          , n = {
                            value: this.getInput().value.trim()
                        };
                        return this.linkValidation && (n.value = this.checkLinkFormat(n.value)),
                        n.target = "_self",
