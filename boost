                this.isPaused = !1,
                this.isSpeaking = !1,
                this.DATABASE = new dn,
                this.voices = [],
                this.config = {
                    wpm: 1,
                    pitch: 1,
                    volume: 1,
                    rate: 1,
                    color: "#000000",
