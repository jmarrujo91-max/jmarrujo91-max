                        text: t,
                        voice: this.TTS_ENGINE.voices()[this.config.videoIndex],
                        force: !0,
                        infiniteResume: !1,
                        volume: this.config.volume,
                        pitch: this.config.pitch,
                        rate: this.config.rate
                    };
