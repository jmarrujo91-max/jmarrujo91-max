        class Mr {
            constructor() {
                this.x = 0,
                this.y = 0,
                this.w = 0,
                this.h = 0
            }
            set x1(t) {
                this.w = this.x2 - t,
                this.x = t
            }
            set y1(t) {
                this.h = this.y2 - t,
                this.y = t
            }
            get x2() {
                return this.x + this.w
            }
            set x2(t) {
                this.w = t - this.x
            }
            get y2() {
                return this.y + this.h
            }
            get aspect() {
                return this.w / this.h
            }
            set y2(t) {
                this.h = t - this.y
            }
            round() {
                return Mr.create(Math.round(this.x), Math.round(this.y), Math.round(this.w), Math.round(this.h))
            }
            normalize() {
                const [t,e,n,r] = [Math.min(this.x, this.x2), Math.min(this.y, this.y2), Math.max(this.x, this.x2), Math.max(this.y, this.y2)];
                return Mr.create(t, e, n - t, r - e)
            }
            rebound(t, e) {
                const n = this.normalize();
                return n.x < 0 && (n.x = 0),
                n.y < 0 && (n.y = 0),
                n.x2 > t && (n.x = t - n.w),
                n.y2 > e && (n.y = e - n.h),
                n
            }
            scale(t, e) {
                return e = e || t,
                Mr.create(this.x * t, this.y * e, this.w * t, this.h * e)
            }
            unscale(t, e) {
                return e = e || t,
                Mr.create(this.x / t, this.y / e, this.w / t, this.h / e)
            }
            center(t, e) {
                return Mr.create((t - this.w) / 2, (e - this.h) / 2, this.w, this.h)
            }
            toArray() {
                return [this.x, this.y, this.w, this.h]
            }
        }
        Mr.fromPoints = function(t, e) {
            const [n,r,i,o] = [Math.min(t[0], e[0]), Math.min(t[1], e[1]), Math.max(t[0], e[0]), Math.max(t[1], e[1])];
