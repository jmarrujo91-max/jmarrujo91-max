            }
            capture() {
                return oi(this, null, (function*() {
                    var t;
                    if (this.selection) {
                        this.stage.destroy();
                        let e = yield Ot({
                            app: "capture",
                            area: this.selection,
                            dpr: window.devicePixelRatio
                        });
                        if (ii(ai.prototype, this, "toaster").call(this, ht("ocr_extracting"), "success"),
                        pt(e.images)) {
