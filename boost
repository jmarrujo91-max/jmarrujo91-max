        const Xr = Yr;
        const Zr = class extends Gr {
            constructor(t, e) {
                super(t, e),
                this.scalex = 1,
                this.scaley = 1,
                this.crops = new Set,
                this.active = null,
                this.enabled = !0,
                this.init()
            }
            init() {
                this.initStageDrag(),
                qr.Manager.attach(this)
            }
            initOptions() {
                this._optconf.multi = t=>{
                    t || this.limitWidgets()
                }
            }
            setEnabled(t=!0) {
                const e = this.options.disabledClass || "jcrop-disable";
                return this[t ? "removeClass" : "addClass"](e),
                this.enabled = !!t,
                this
            }
            focus() {
                return !!this.enabled && (this.active ? this.active.el.focus() : this.el.focus(),
                this)
            }
            limitWidgets(t=1) {
                if (!this.crops || t < 1)
                    return !1;
                const e = Array.from(this.crops);
                for (; e.length > t; )
                    this.removeWidget(e.shift());
                return this
            }
            canCreate() {
                const t = this.crops.size
                  , e = this.options;
                return !!this.enabled && (!(null !== e.multiMax && t >= e.multiMax) && !(!e.multi && t >= e.multiMin))
            }
            canRemove() {
                const t = this.crops.size
                  , e = this.options;
                return !!this.enabled && (!(this.active && !this.active.options.canRemove) && !(!e.canRemove || t <= e.multiMin))
            }
            initStageDrag() {
                var t, e, n, r, i;
                Nr(this.el, ((o,a,s)=>!!this.canCreate() && (t = (this.options.widgetConstructor || Xr).create(this.options),
                (e = t.pos).x = s.pageX - this.el.offsetParent.offsetLeft - this.el.offsetLeft,
                e.y = s.pageY - this.el.offsetParent.offsetTop - this.el.offsetTop,
                n = this.el.offsetWidth,
                r = this.el.offsetHeight,
                this.addWidget(t),
                i = $r.create(e, n, r, "se"),
                this.options.aspectRatio && (i.aspect = this.options.aspectRatio),
                t.render(e),
                this.focus(),
                !0)), ((e,n)=>{
                    t.render(i.move(e, n))
                }
                ), (()=>{
                    t.emit("crop.change")
                }
                ))
            }
            reorderWidgets() {
                var t = 10;
                this.crops.forEach((e=>{
                    e.el.style.zIndex = t++,
                    this.active === e ? e.addClass("active") : e.removeClass("active")
                }
                )),
                this.refresh()
            }
            activate(t) {
                if (!this.enabled)
                    return this;
                if (t = t || Array.from(this.crops).pop()) {
                    if (this.active === t)
                        return;
                    this.active = t,
                    this.crops.delete(t),
                    this.crops.add(t),
                    this.reorderWidgets(),
                    this.active.el.focus(),
                    this.options.shade && this.shades.enable(),
                    t.emit("crop.activate")
                } else
                    this.shades.disable();
                return this
            }
            addWidget(t) {
                return t.attachToStage(this),
                t.appendTo(this.el),
                this.activate(t),
                this
            }
            newWidget(t, e={}) {
                e = jr({}, this.options, e);
                const n = (this.options.widgetConstructor || Xr).create(e);
                return n.render(t.unscale(this.scalex, this.scaley)),
                this.addWidget(n),
                n.el.focus(),
                n
            }
            removeWidget(t) {
                if (!this.canRemove())
                    return !1;
                t.emit("crop.remove"),
                t.el.remove(),
                this.crops.delete(t),
                this.activate()
            }
            refresh() {
                this.crops.forEach((t=>{
                    t.render()
                }
                )),
                this.options.shade && this.active && this.shades.adjust(this.active.pos)
            }
            updateShades() {
                if (this.shades)
                    return this.options.shade ? this.shades.enable() : this.shades.disable(),
                    this.options.shade && this.active && this.shades.adjust(this.active.pos),
                    this
            }
            setOptions(t={}) {
                super.setOptions(t),
                this.crops && Array.from(this.crops).forEach((e=>e.setOptions(t)))
            }
            destroy() {}
        }
        ;
        const Jr = class extends Zr {
            constructor(t, e) {
                const n = function(t, e=document.createElement("div")) {
                    return e.className = t,
                    e
                }("jcrop-stage jcrop-image-stage");
                t.parentNode.insertBefore(n, t),
                n.appendChild(t),
