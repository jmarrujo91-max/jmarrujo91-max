            crop(t, e) {
                return oi(this, arguments, (function*(t, e, n=window.devicePixelRatio, r, i) {
                    return new Promise((o=>{
                        let a = e.y * n
                          , s = e.x * n
                          , l = e.w * n
                          , c = e.h * n
                          , d = 1 !== n && r ? l : e.w
                          , h = 1 !== n && r ? c : e.h
                          , p = null;
                        p || (p = document.createElement("canvas"),
                        document.body.appendChild(p)),
                        p.width = d,
                        p.height = h,
                        p.style.display = "none";
                        let u = new Image;
                        u.style.display = "none",
