                    yield this.HIGHLIGHT_MENU.load(this.isRunning, this.isPaid),
                    e
                }
                ))
            }
            features() {
                return {
                    AUTO_SCROLL: this.AUTO_SCROLL,
                    CUSTOM_FONT: this.CUSTOM_FONT,
