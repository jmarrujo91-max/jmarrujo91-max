                    yield this.STICKY_NOTES.load(this.FIRST_LOAD, this.isPaid)),
                    yield this.WORD_PREDICTION.load(this.isRunning, this.isPaid),
                    yield this.READER_MODE.load(this.isPaid),
                    yield this.CUSTOM_FONT.load(this.isRunning, this.isPaid),
                    yield this.HIGHLIGHT_MENU.load(this.isRunning, this.isPaid),
                    yield this.TTS.load(this.isRunning, this.isPaid),
                    this.FIRST_LOAD = !1,
                    !0
                }
                ))
            }
            handleSaveToReadingList() {
                return Fo(this, null, (function*() {
                    const t = Tt(document)
                      , e = yield Ot(t)
                      , n = yield ht(e.message)
                      , r = e.messageType || "success";
                    return Oo(Mo.prototype, this, "toaster").call(this, n, r),
                    e
                }
                ))
            }
            handleImmersiveReader(t) {
                return Fo(this, null, (function*() {
                    let e = null;
                    if (pt(t.text)) {
                        const e = yield this.getSelectedText()
                          , n = "object" == typeof e.text;
                        t.text = n ? e.text.html : e.text
                    }
                    const n = yield ht("popup_immersive_success");
                    Oo(Mo.prototype, this, "toaster").call(this, n, "success");
                    return e = yield this.IMMERSIVE.start(t.text),
                    e
                }
                ))
            }
            handleReaderMode(t) {
                return Fo(this, null, (function*() {
                    const e = yield this.READER_MODE.on(t, "selected")
                      , n = yield ht(e.message)
                      , r = e.messageType || "success";
