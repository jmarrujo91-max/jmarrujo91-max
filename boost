                        this)
                    },
                    positionPreview: function(t) {
                        t = t || this.activeAnchor;
                        var e, n, r, i, o, a = this.window.innerWidth, s = this.anchorPreview.offsetHeight, l = t.getBoundingClientRect(), c = this.diffLeft, d = this.diffTop, h = this.getEditorOption("elementsContainer"), p = ["absolute", "fixed"].indexOf(window.getComputedStyle(h).getPropertyValue("position")) > -1, u = {};
                        e = this.anchorPreview.offsetWidth / 2;
