                        t.join("")
                    },
                    isDisplayed: function() {
                        return t.extensions.form.prototype.isDisplayed.apply(this)
                    },
                    hideForm: function() {
                        t.extensions.form.prototype.hideForm.apply(this),
                        this.getInput().value = ""
                    },
                    showForm: function(e) {
