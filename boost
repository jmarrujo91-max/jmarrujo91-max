            unescapeString(t) {
                var e = document.createElement("div");
                return e.innerHTML = t,
                e.textContent
            }
            convertMathMLString(t) {
                try {
                    let e = (new DOMParser).parseFromString(t, "application/xml");
                    if (e.querySelector("parsererror"))
                        throw new Error("Parsing error");
                    return (new XMLSerializer).serializeToString(e)
                } catch (t) {
                    return console.log("Error in convertMathMLString:", t),
                    "Issue with the maths reader beta, please try again"
                }
            }
