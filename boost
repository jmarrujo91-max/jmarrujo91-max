            addEventListeners() {
                this.shadowElement.shadowRoot.getElementById("close_banner").addEventListener("click", (t=>{
                    t.preventDefault(),
                    this.handleClose()
                }
                )),
                this.shadowElement.shadowRoot.getElementById("copy_banner").addEventListener("click", (t=>{
                    t.preventDefault(),
                    this.handleCopy()
                }
                )),
                this.shadowElement.shadowRoot.getElementById("start_banner").addEventListener("click", (t=>{
                    t.preventDefault(),
                    this.handleStart()
                }
                ))
            }
            handleClose() {
                this.toggleMicrophoneStatus(!1),
                this.fullTranscript = "",
                this.shadowElement.shadowRoot.getElementById("start_result").innerHTML = "",
                clearTimeout(this.timeout),
                Et(this.id),
                this.recognizing && this.stopRecognition()
            }
            handleCopy() {
                let t = this.shadowElement.shadowRoot.getElementById("start_result").innerHTML;
                navigator.clipboard.writeText(t).then((()=>{
                    let t = ht("copied_to_clipboard");
                    super.toaster(t, "success")
                }
                )).catch((t=>{
                    console.warn("🦉 Helperbird  warn: ", t);
                    let e = ht("copied_to_clipboard");
                    super.toaster(e, "error")
                }
                ))
            }
            handleStart() {
                this.recognizing ? this.stopRecognition() : (this.isSentence = this.prefix.trim().slice(-1).match(/[\.\?\!]$/),
                this.recognition.start(),
                this.recognition.onstart = ()=>bo(this, null, (function*() {
                    this.startRecognition()
                }
                )),
                this.recognition.onend = ()=>bo(this, null, (function*() {
                    this.endRecognition()
                }
                )),
                this.recognition.onresult = t=>{
                    this.handleRecognitionResult(t)
                }
                )
            }
            startRecognition() {
                this.recognizing = !0,
                this.toggleMicrophoneStatus(!0),
                this.restartTimer();
                let t = ht("voice_typing_start");
                super.toaster(t, "success")
            }
            endRecognition() {
                this.recognizing = !1,
                this.toggleMicrophoneStatus(!1),
                clearTimeout(this.timeout);
                let t = ht("voice_typing_stopped");
                super.toaster(t, "success")
            }
            handleRecognitionResult(t) {
                clearTimeout(this.timeout);
                let e = t.results
                  , n = ""
                  , r = "";
                for (let i = t.resultIndex; i < e.length; ++i) {
                    let t = e[i]
                      , o = t[0];
                    t.isFinal ? (n = this.formatTranscript(o.transcript),
                    this.fullTranscript += n) : r += this.formatTranscript(o.transcript)
                }
                let i = this.fullTranscript + r;
                this.isSentence && (i = i.charAt(0).toUpperCase() + i.slice(1)),
                this.shadowElement.shadowRoot.getElementById("start_result").innerHTML = i,
                this.restartTimer()
            }
            stopRecognition() {
                this.toggleMicrophoneStatus(!1),
                this.recognition.stop()
            }
