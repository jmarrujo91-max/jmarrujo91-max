                    let a = Nt() || Lt() ? "display:none" : "display:block";
                    this.shadow.shadowRoot.innerHTML = `\n      <style id="helperbird-reader-mode-css">\t\n\n\n  \n\n\n          \n     \n\n        ${ve.toString()}\n        ${'.tw-helperbird-app-prose {\n  color: var(--tw-prose-body);\n  max-width: 65ch;\n}\n.tw-helperbird-app-prose :where(a):not(:where([class~="not-prose"] *)) {\n  color: var(--tw-prose-links);\n  text-decoration: underline;\n  font-weight: 500;\n}\n.tw-helperbird-app-prose :where(strong):not(:where([class~="not-prose"] *)) {\n  color: var(--tw-prose-bold);\n  font-weight: 600;\n}\n.tw-helperbird-app-prose :where(ul):not(:where([class~="not-prose"] *)) {\n  list-style-type: disc;\n  padding-left: 1.625em;\n}\n.tw-helperbird-app-prose\n  :where(blockquote):not(:where([class~="not-prose"] *)) {\n  font-weight: 500;\n  font-style: italic;\n  color: var(--tw-prose-quotes);\n  border-left-width: 0.25rem;\n  border-left-color: var(--tw-prose-quote-borders);\n  quotes: "\\201C""\\201D""\\2018""\\2019";\n  margin-top: 1.6em;\n  margin-bottom: 1.6em;\n  padding-left: 1em;\n}\n.tw-helperbird-app-prose\n  :where(blockquote p:first-of-type):not(\n    :where([class~="not-prose"] *)\n  )::before {\n  content: open-quote;\n}\n.tw-helperbird-app-prose\n  :where(blockquote p:last-of-type):not(:where([class~="not-prose"] *))::after {\n  content: close-quote;\n}\n.tw-helperbird-app-prose :where(h2):not(:where([class~="not-prose"] *)) {\n  color: var(--tw-prose-headings);\n  font-weight: 700;\n  font-size: 1.5em;\n  margin-top: 2em;\n  margin-bottom: 1em;\n  line-height: 1.3333333;\n}\n.tw-helperbird-app-prose\n  :where(figure > *):not(:where([class~="not-prose"] *)) {\n  margin-top: 0;\n  margin-bottom: 0;\n}\n.tw-helperbird-app-prose\n  :where(figcaption):not(:where([class~="not-prose"] *)) {\n  color: var(--tw-prose-captions);\n  font-size: 0.875em;\n  line-height: 1.4285714;\n  margin-top: 0.8571429em;\n}\n.tw-helperbird-app-prose {\n  --tw-prose-body: #374151;\n  --tw-prose-headings: #111827;\n  --tw-prose-lead: