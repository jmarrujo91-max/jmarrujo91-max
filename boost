                return Mr.fromArray(t);
            const e = new Mr;
            return e.x = t.offsetLeft,
            e.y = t.offsetTop,
            e.w = t.offsetWidth,
            e.h = t.offsetHeight,
            e
        }
        ,
        Mr.fromArray = function(t) {
            if (4 === t.length)
                return Mr.create.apply(this, t);
            if (2 === t.length)
                return Mr.fromPoints(t[0], t[1]);
            throw new Error("fromArray method problem")
        }
        ,
        Mr.sizeOf = function(t, e) {
            if (e)
                return Mr.create(0, 0, t, e);
            const n = new Mr;
            return n.w = t.offsetWidth,
            n.h = t.offsetHeight,
            n
        }
        ,
        Mr.getMax = function(t, e, n) {
            return t / e > n ? [e * n, e] : [t, t / n]
        }
        ,
        Mr.fromPoint = function(t, e, n, r="br") {
            const i = new Mr;
            switch (i.x = t[0],
            i.y = t[1],
            r) {
            case "br":
                i.x2 = i.x + e,
                i.y2 = i.y + n;
                break;
            case "bl":
                i.x2 = i.x - e,
                i.y2 = i.y + n;
                break;
            case "tl":
                i.x2 = i.x - e,
                i.y2 = i.y - n;
                break;
            case "tr":
                i.x2 = i.x + e,
                i.y2 = i.y - n
            }
            return i
        }
        ;
        const Br = Mr;
        class zr {
            constructor(t) {
                "string" == typeof t && (t = document.getElementById(t)),
                this.el = t,
                this.shades = {}
            }
            init(t={}) {
                this.active = void 0 === t.shade || t.shade,
                this.keys().forEach((e=>{
                    this.shades[e] = Hr.create(t, e)
                }
                )),
                this.el.addEventListener("crop.update", (t=>{
                    t.cropTarget.isActive() && t.cropTarget.options.shade && this.adjust(t.cropTarget.pos)
                }
                ), !1),
                this.enable()
            }
            adjust(t) {
                const e = Br.from(this.el)
                  , n = this.shades;
                n.t.h = t.y,
                n.b.h = e.h - t.y2,
                n.t.w = n.b.w = Math.floor(t.w),
                n.l.w = n.t.x = n.b.x = Math.ceil(t.x),
                n.r.w = e.w - (Math.ceil(t.x) + Math.floor(t.w))
            }
            keys() {
                return ["t", "l", "r", "b"]
            }
            enable() {
                const t = this.shades;
                this.keys().forEach((e=>t[e].insert(this.el)))
            }
            disable() {
                const t = this.shades;
                this.keys().forEach((e=>t[e].remove()))
            }
            setStyle(t, e) {
                const n = this.shades;
                this.keys().forEach((r=>n[r].color(t).opacity(e)))
            }
        }
        zr.attach = function(t) {
            const e = t.el
              , n = new zr(e);
            return n.init(t.options),
            t.shades = n,
            t._optconf.shade = e=>t.updateShades(),
            t._optconf.shadeColor = t=>n.setStyle(t),
            t._optconf.shadeOpacity = t=>n.setStyle(null, t),
            n
        }
        ;
        class Hr extends Dr {
            insert(t) {
                t.appendChild(this.el)
            }
            remove() {
                this.el.remove()
            }
            set w(t) {
                this.el.style.width = t + "px"
            }
            set h(t) {
                this.el.style.height = t + "px"
            }
            set x(t) {
                this.el.style.left = t + "px"
            }
            color(t) {
                return t && (this.el.style.backgroundColor = t),
                this
            }
            opacity(t) {
                return t && (this.el.style.opacity = t),
                this
            }
        }
        Hr.create = function(t, e) {
            const n = document.createElement("div")
              , r = t.shadeClass || "jcrop-shade";
            n.className = `${r} ${e}`;
            return new Hr(n).color(t.shadeColor).opacity(t.shadeOpacity)
        }
        ,
        Hr.Manager = zr;
        const qr = Hr;
        function jr() {
            var t = {};
            for (let n in arguments) {
                var e = arguments[n];
                for (let n in e)
                    Object.prototype.hasOwnProperty.call(e, n) && (t[n] = e[n])
            }
            return t
        }
        class Ur {
            constructor(t, e, n, r) {
                this.sw = e,
                this.sh = n,
                this.rect = t,
                this.locked = this.getCornerPoint(this.getOppositeCorner(r)),
                this.stuck = this.getCornerPoint(r)
            }
            move(t, e) {
                return Br.fromPoints(this.locked, this.translateStuckPoint(t, e))
            }
            getDragQuadrant(t, e) {
                const n = this.locked[0] - t
                  , r = this.locked[1] - e;
                return n < 0 && r < 0 ? "br" : n >= 0 && r >= 0 ? "tl" : n < 0 && r >= 0 ? "tr" : "bl"
            }
            getMaxRect(t, e, n) {
                return Br.getMax(Math.abs(this.locked[0] - t), Math.abs(this.locked[1] - e), n)
            }
            translateStuckPoint(t, e) {
                const [n,r,i] = this.stuck;
                var o = null === n ? i : n + t
                  , a = null === r ? i : r + e;
                if (o > this.sw && (o = this.sw),
                a > this.sh && (a = this.sh),
                o < 0 && (o = 0),
                a < 0 && (a = 0),
                this.aspect) {
                    var [s,l] = this.getMaxRect(o, a, this.aspect)
                      , c = this.getDragQuadrant(o, a)
                      , d = Br.fromPoint(this.locked, s, l, c);
                    return [d.x2, d.y2]
                }
                return [o, a]
            }
            getCornerPoint(t) {
                const e = this.rect;
                switch (t) {
                case "n":
                    return [null, e.y, e.x];
                case "s":
                    return [null, e.y2, e.x2];
                case "e":
                    return [e.x2, null, e.y2];
                case "w":
                    return [e.x, null, e.y];
                case "se":
                    return [e.x2, e.y2];
                case "sw":
                    return [e.x, e.y2];
                case "ne":
                    return [e.x2, e.y];
                case "nw":
                    return [e.x, e.y]
                }
            }
            getOppositeCorner(t) {
                switch (t) {
                case "n":
                case "w":
                case "nw":
                    return "se";
                case "s":
                case "e":
                case "se":
                    return "nw";
                case "sw":
                    return "ne";
                case "ne":
                    return "sw"
                }
            }
        }
        Ur.create = function(t, e, n, r) {
            return new Ur(t,e,n,r)
        }
        ;
        const $r = Ur;
        const Gr = class extends Dr {
            constructor(t, e={}) {
                super(t),
                this.options = {},
                Object.defineProperty(this, "_optconf", {
                    configurable: !1,
                    enumerable: !1,
                    value: {},
                    writable: !0
                }),
                this.initOptions(),
                this.setOptions(jr({}, Sr, e))
            }
            setOptions(t) {
                return this.options = jr({}, this.options, t),
                Object.keys(t).forEach((e=>{
                    this._optconf[e] && this._optconf[e](t[e])
                }
                )),
                this
            }
            initOptions() {}
        }
        ;
        class Vr {
            constructor(t) {
                this.widget = t,
                this.attach()
            }
            attach() {
                const t = this.widget;
                t.el.addEventListener("keydown", (e=>{
                    const n = e.shiftKey ? 10 : 1;
                    switch (e.key) {
                    case "ArrowRight":
                        t.nudge(n);
                        break;
                    case "ArrowLeft":
                        t.nudge(-n);
                        break;
                    case "ArrowUp":
                        t.nudge(0, -n);
                        break;
                    case "ArrowDown":
                        t.nudge(0, n);
                        break;
                    case "Delete":
                    case "Backspace":
                        t.stage.removeWidget(t);
                        break;
                    default:
                        return
                    }
                    e.preventDefault()
                }
                ))
            }
        }
        Vr.attach = function(t) {
            return new Vr(t)
        }
        ;
        const Wr = Vr;
        const Kr = function(t, e, n, r, i=30, o="swing") {
            const a = ["x", "y", "w", "h"]
              , s = e.normalize();
            o = "string" == typeof o ? Ir()[o] : o;
            var l = 0;
            return new Promise(((t,c)=>{
                requestAnimationFrame((function c() {
                    l < i ? (a.forEach((t=>{
                        s[t] = Math.round(o(l, e[t], n[t] - e[t], i))
                    }
                    )),
                    r(s),
                    l++,
                    requestAnimationFrame(c)) : (r(n),
                    t())
                }
                ))
            }
            ))
        };
        class Yr extends Gr {
            constructor(t, e={}) {
                super(t, e),
                this.pos = Br.from(this.el),
                this.init()
            }
            init() {
                return this.createHandles(),
                this.createMover(),
                this.attachFocus(),
                Wr.attach(this),
                this
            }
            initOptions() {
                this._optconf.aspectRatio = t=>{
                    const e = this.pos;
                    if (this.aspect = t || null,
                    this.aspect && e) {
                        var [n,r] = Br.getMax(e.w, e.h, t);
                        this.render(Br.fromPoint([e.x, e.y], n, r))
                    }
                }
            }
            attachToStage(t) {
                this.stage = t,
                this.emit("crop.attach")
            }
            attachFocus() {
                this.el.addEventListener("focus", (t=>{
                    this.stage.activate(this),
                    this.emit("crop.update")
                }
                ), !1)
            }
            animate(t, e, n) {
                const r = this;
                return n = n || r.options.animateEasingFunction || "swing",
                e = e || r.options.animateFrames || 30,
                Kr(r.el, r.pos, t, (t=>r.render(t.normalize())), e, n).then((()=>this.emit("crop.change")))
            }
            createMover() {
                var t, e, n;
                this.pos = Br.from(this.el),
                Nr(this.el, (()=>{
                    const r = this.el.parentElement;
                    return !!this.stage.enabled && ([t,e] = [r.offsetWidth, r.offsetHeight],
                    n = Br.from(this.el),
                    this.el.focus(),
                    this.stage.activate(this),
                    !0)
                }
                ), ((r,i)=>{
                    this.pos.x = n.x + r,
                    this.pos.y = n.y + i,
                    this.render(this.pos.rebound(t, e))
                }
                ), (()=>{
                    this.emit("crop.change")
                }
                ))
            }
            nudge(t=0, e=0) {
                const n = this.el.parentElement
                  , [r,i] = [n.offsetWidth, n.offsetHeight];
                t && (this.pos.x += t),
                e && (this.pos.y += e),
                this.render(this.pos.rebound(r, i)),
                this.emit("crop.change")
            }
            createHandles() {
                return this.options.handles.forEach((t=>{
                    const e = Pr.create("jcrop-handle " + t);
                    var n;
                    e.appendTo(this.el),
                    Nr(e.el, (()=>{
                        if (!this.stage.enabled)
                            return !1;
                        const e = this.el.parentElement
                          , r = e.offsetWidth
                          , i = e.offsetHeight;
                        return n = $r.create(Br.from(this.el), r, i, t),
                        this.aspect && (n.aspect = this.aspect),
                        this.el.focus(),
                        this.emit("crop.active"),
                        !0
                    }
                    ), ((t,e)=>this.render(n.move(t, e))), (()=>{
                        this.emit("crop.change")
                    }
                    ))
                }
                )),
                this
            }
            isActive() {
                return this.stage && this.stage.active === this
            }
            get sel() {
                const t = this.stage;
                return this.pos.scale(t.scalex, t.scaley)
            }
            render(t) {
                return t = t || this.pos,
                this.el.style.top = Math.round(t.y) + "px",
                this.el.style.left = Math.round(t.x) + "px",
                this.el.style.width = Math.round(t.w) + "px",
                this.el.style.height = Math.round(t.h) + "px",
                this.pos = t,
                this.emit("crop.update"),
                this
            }
            doneDragging() {
                this.pos = Br.from(this.el)
            }
        }
        Yr.create = function(t={}) {
            const e = document.createElement("div")
              , n = jr({}, Sr, t);
            return e.setAttribute("tabindex", "0"),
            e.className = n.cropperClass || "jcrop-widget",
            new (t.widgetConstructor || Yr)(e,n)
        }
