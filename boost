                            "function" == typeof t.checkState ? t.checkState(e) : "function" == typeof t.isActive && "function" == typeof t.isAlreadyApplied && "function" == typeof t.setActive && !t.isActive() && t.isAlreadyApplied(e) && t.setActive()
