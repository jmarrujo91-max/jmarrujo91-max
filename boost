                        on: function(t, e, n, r) {
                            return this.events.attachDOMEvent(t, e, n, r),
                            this
                        },
                        off: function(t, e, n, r) {
                            return this.events.detachDOMEvent(t, e, n, r),
                            this
                        },
                        subscribe: function(t, e) {
                            return this.events.attachCustomEvent(t, e),
                            this
                        },
                        unsubscribe: function(t, e) {
