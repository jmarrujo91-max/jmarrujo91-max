                }
            }
            worker(t) {
                return Fo(this, null, (function*() {
                    return new Promise((e=>{
                        let n = t.popup;
                        if ("immersive_reader" === n) {
                            return e(this.handleImmersiveReader(t))
                        }
                        if ("save-to-reading-list" === n) {
                            return e(this.handleSaveToReadingList())
                        }
                        if ("voice_typing_on" === n) {
                            return e(this.VOICE_TYPING.on())
                        }
                        if ("reader_mode" === n) {
                            return e(this.handleReaderMode(t.text))
                        }
                        if ("read" === n) {
                            return e(this.TTS.speak(t.text))
                        }
                        if ("stop" === n) {
                            return e(this.TTS.stop(t.text))
                        }
                        if ("tts" === n) {
                            return e(this.TTS.speak(t.text))
                        }
                        if ("selected" === n) {
                            return e(this.getSelectedText())
                        }
                        if ("alert" === n) {
                            return e(Oo(Mo.prototype, this, "toaster").call(this, t.message, t.type))
                        }
                        if ("scroll" === n) {
                            return e(this.AUTO_SCROLL.on())
                        }
                        if ("annotate_page" === n) {
                            return e(this.STICKY_NOTES.create())
                        }
                        if ("ocr-inject-maths" === n) {
                            let t = !1;
                            return e(this.OCR.on(t))
                        }
                        if ("ocr-inject" === n) {
                            let t = !0;
                            return e(this.OCR.on(t))
                        }
                        if ("ocr-inject" === n) {
                            let t = !0;
                            return e(this.OCR.on(t))
                        }
                        if ("refresh" === n) {
                            return e(this.refresh())
                        }
                        if ("ocr-done" === n)
                            return Oo(Mo.prototype, this, "toaster").call(this, t.message, t.type),
                            Dt(t.ocrText),
                            e({
                                done: !0
                            });
                        if ("reboot" === n) {
                            return e(this.DATABASE.reboot())
                        }
                        if ("print" === n) {
                            return e(this.PRINT.on())
                        }
                        return e({
                            error: !0
                        })
                    }
                    ))
                }
                ))
            }
            start() {
                this.debouncedRefresh = function(t, e) {
                    let n;
                    return function(...r) {
                        clearTimeout(n),
                        n = setTimeout((()=>{
                            clearTimeout(n),
                            t(...r)
                        }
                        ), e)
                    }
                }(this.refresh.bind(this), C.refresh),
                chrome.storage.onChanged.addListener((()=>Fo(this, null, (function*() {
                    this.debouncedRefresh()
                }
                )))),
                chrome.runtime.onMessage.addListener(((t,e,n)=>((()=>{
                    Fo(this, null, (function*() {
                        try {
                            const r = yield this.worker(t, e);
                            n(r)
                        } catch (t) {
                            n({
                                error: "An error occurred"
                            })
                        }
                    }
                    ))
                }
                )(),
                !0)))
            }
            getSelectedText() {
                return Fo(this, null, (function*() {
                    return {
                        app: "alert",
                        popup: "alert",
                        message: "getting_text",
                        replacement: "",
                        text: pt(window.getSelection().toString()) ? yield be() : window.getSelection().toString(),
                        messageType: "success",
                        error: !1
                    }
                }
                ))
            }
            pdfCheck() {
                return Fo(this, null, (function*() {
                    if (!0 === ("undefined" != typeof window && window.location && window.location.href.endsWith(".pdf"))) {
                        let t = yield ht("helperbird_pdf_open", "Helperbirds")
                          , e = encodeURIComponent(window.location.href);
                        Oo(Mo.prototype, this, "toaster").call(this, t, "success", `pdf.html?helperbirdPDF=${e}`)
                    }
                }
                ))
            }
            on() {
                return Fo(this, null, (function*() {
                    this.isRunning = yield this.DATABASE.isRunning(),
                    this.isPaid = yield this.paidCheck(),
                    yield this.pistons();
                    const t = {
                        app: "update-badge",
                        state: this.isRunning
                    };
                    yield Ot(t),
                    this.FIRST_LOAD = !1
                }
                ))
            }
            refresh() {
                return Fo(this, null, (function*() {
                    return this.HOTKEYS.unbind(),
                    yield this.DATABASE.load(),
                    yield this.pdfCheck(),
                    this.on(),
                    {
                        isRefresh: !0
                    }
                }
                ))
            }
        }
        window.HELPERBIRD_ENGINE = !0,
        new Mo,
        console.log("🦉 Helperbird loaded")
    }
    )()
}
)();
