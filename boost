                    videoIndex: 0
                }
            }
            getWordStart(t, e) {
                return t = String(t),
                e = Number(e) >>> 0,
                t.slice(0, e + 1).search(/\S+$/)
            }
            getWordAt(t, e) {
                t = String(t),
                e = Number(e) >>> 0;
                var n = t.slice(0, e + 1).search(/\S+$/)
                  , r = t.slice(e).search(/\s/);
                return r < 0 ? t.slice(n) : t.slice(n, r + e)
            }
            onboundaryHandler(t) {
                var e = document.getElementById("helperbird-description-input");
                if (!e)
                    return !1;
                var n = e.scrollTop
                  , r = e.value
                  , i = t.charIndex
                  , o = this.getWordAt(r, i)
                  , a = this.getWordStart(r, i)
                  , s = a + o.length;
                if (e.focus(),
                e.setSelectionRange)
                    e.setSelectionRange(a, s);
                else {
                    var l = e.createTextRange();
                    l.collapse(!0),
                    l.moveEnd("character", s),
                    l.moveStart("character", a),
                    l.select()
                }
                e.scrollTop = n
            }
            debug() {
                return fn(this, null, (function*() {
                    return this.TTS_ENGINE.on({
                        boundary: t=>{
                            this.onboundaryHandler(t)
                        }
                        ,
                        start: ()=>{
                            this.getStatus()
                        }
                        ,
                        end: ()=>{
                            this.getStatus()
                        }
                        ,
                        error: ()=>{
                            this.getStatus()
                        }
                        ,
                        pause: ()=>{
                            this.getStatus()
                        }
                        ,
                        resume: ()=>{
                            this.getStatus()
                        }
                    }),
                    !0
                }
                ))
            }
            refresh() {
                return fn(this, null, (function*() {
                    yield this.DATABASE.load();
                    let t = this.DATABASE
                      , e = this.TTS_ENGINE.detect();
                    return this.isSupported = void 0 !== e.speechSynthesis && void 0 !== e.speechSynthesisUtterance,
                    this.config.wpm = t.hasProperty("wpm", this.config.wpm),
                    this.config.pitch = t.hasProperty("pitch", this.config.pitch),
                    this.config.volume = t.hasProperty("volume", this.config.volume),
                    this.config.voice = t.hasProperty("voice", this.voices[0]),
                    this.config.rate = t.hasProperty("rate", this.config.rate),
                    this.config.color = t.hasProperty("textToSpeechColor", this.config.color),
                    this.config.videoIndex = this.voices.findIndex((t=>t.name === this.config.voice)),
                    -1 === this.config.videoIndex ? this.config.videoIndex = 0 : this.config.voice = this.voices[this.config.videoIndex],
                    !0
                }
                ))
            }
            load() {
                return fn(this, null, (function*() {
                    return yield this.initializeEasySpeech(),
                    yield this.refresh(),
                    yield this.debug(),
                    !0
                }
                ))
            }
            initializeEasySpeech() {
                return fn(this, null, (function*() {
                    try {
                        yield this.TTS_ENGINE.init({
                            maxTimeout: 5e3,
                            interval: 250,
                            quiet: !1
                        }),
                        this.voices = this.TTS_ENGINE.voices()
                    } catch (t) {
                        return !1
                    }
                    return !0
                }
                ))
            }
            speak(t) {
                return fn(this, null, (function*() {
                    yield this.refresh();
                    let e = {
                        message: ht("playing"),
                        type: "success"
                    };
                    un(mn.prototype, this, "toaster").call(this, e.message, e.type);
