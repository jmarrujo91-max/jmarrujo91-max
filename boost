                            for (var i = 0; i < n.length; i++)
                                r = this.removeObsoleteAutoLinkSpans(n[i]) || r,
                                r = this.performLinkingWithinElement(n[i]) || r;
                            return this.base.events.updateInput(e, {
                                target: e,
                                currentTarget: e
                            }),
                            r
                        },
                        removeObsoleteAutoLinkSpans: function(e) {
                            if (!e || 3 === e.nodeType)
                                return !1;
