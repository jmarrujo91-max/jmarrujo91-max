                        var t = null;
                        return this.useQueryState && (t = this.base.queryCommandState(this.getAction())),
                        t
                    },
                    isAlreadyApplied: function(t) {
                        var e, n, r = !1, i = this.getTagNames();
                        return !1 === this.knownState || !0 === this.knownState ? this.knownState : (i && i.length > 0 && (r = -1 !== i.indexOf(t.nodeName.toLowerCase())),
